{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/authenticate",
        "dest": "src/app.js"
      },
      {
        "src": "/callback",
        "dest": "src/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/app.js" 
      }
    ]
  }
  